{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@arvinxu/modules-foo": "./src"
        }
      }
    ],
    "@babel/plugin-transform-react-jsx"
  ]
}
